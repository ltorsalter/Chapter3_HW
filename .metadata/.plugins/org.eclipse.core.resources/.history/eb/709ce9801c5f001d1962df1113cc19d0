
public class test_p09 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Car4th car1;
		car1=new Car4th();
		
		car1.num=1234;
		car1.gas=20.5;
	}
}

class Car4th{
	int num;
	double gas;
	
	void show() {
		System.out.println("車號是"+num);
		System.out.println("汽油量是"+gas);
	}
}